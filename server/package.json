{
  "name": "mern-template-server",
  "version": "1.0.0",
  "description": "Server for MERN template with BetterAuth",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
